{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://rentcar.com/v1/classe.json",
    "type": "object",
    "title": "classe",
    "description": "Schema para o objeto que classifica os veiculos.",
    "required": [
      "id",
      "descricao"
    ],
    "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "O Id da classe",
        "description": "Identificador único.",
        "default": 0,
        "examples": [
          1
        ]
      },
      "descricao": {
        "$id": "#/properties/descricao",
        "type": "string",
        "title": "A Descricao do classe",
        "description": "Descreve a classificação.",
        "minLength": 3,
        "maxLength": 20,
        "examples": [
          "sedan"
        ],
        "pattern": "^(.*)$"
      }
    }
  }